<table cellpadding="0" cellspacing="0" class="bg-color-wrapper account-info-card" width="100%"
       style="height: auto;">
    <tbody>
    <tr>
        <td class="content-padding" style="padding-left: 40.0px;padding-right: 40.0px;">
            <table cellpadding="0" cellspacing="0" class="color-wrapper" width="100%"
                   style="height: auto;">
                <tbody>
                <tr>
                    <td bgcolor="#FFFFFF" class="inner-wrapper drop-shadow-true outer-radius"
                        style="padding-bottom: 0;border: 1.0px solid rgb(234,234,234);">
                        <table cellpadding="0" cellspacing="0" class="account-info table "
                               width="100%" style="height: auto;">
                            <tbody>
                            {% for cel in cels %}
                                {% include 'email/_table_cel.html.twig' with {
                                    title: cel[0],
                                    content: cel[1]
                                }
                                %}
                            {% endfor %}
                            </tbody>
                        </table>
                        {{ include('email/_spacer.html.twig') }}
                    </td>
                </tr>
                </tbody>
            </table>
        </td>
    </tr>
    </tbody>
</table>